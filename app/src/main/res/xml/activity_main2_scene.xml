<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
            motion:constraintSetEnd="@+id/end"
            motion:constraintSetStart="@id/start"
            motion:duration="600" android:id="@+id/baseTransition">
       <KeyFrameSet>
       </KeyFrameSet>
        <OnClick/>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint android:layout_height="fill_parent" motion:layout_constraintTop_toTopOf="parent"
                    motion:layout_constraintHorizontal_bias="0.5" motion:layout_constraintLeft_toLeftOf="parent"
                    motion:layout_constraintRight_toRightOf="parent" android:layout_width="match_parent"
                    android:id="@+id/nav_host_fragment" motion:layout_constraintBottom_toTopOf="@+id/nav_view"
        />
        <Constraint motion:layout_editor_absoluteY="751dp" android:layout_height="wrap_content"
                    motion:layout_constraintLeft_toLeftOf="parent" motion:layout_constraintRight_toRightOf="parent"
                    android:layout_width="0dp" android:id="@+id/nav_view"
                    motion:layout_constraintBottom_toBottomOf="parent"/>
        <Constraint
                android:layout_width="match_parent" android:layout_height="wrap_content"
                    motion:layout_constraintEnd_toEndOf="parent"
                    motion:layout_constraintStart_toStartOf="parent" motion:layout_constraintHorizontal_bias="0.5"
                    android:visibility="visible"
        />
        <Constraint android:id="@+id/frameLayout" motion:layout_constraintEnd_toEndOf="parent"
                    android:layout_width="match_parent" android:layout_height="wrap_content"
                    motion:layout_constraintHorizontal_bias="0.5" motion:layout_constraintStart_toStartOf="parent"
                    android:visibility="invisible" motion:layout_constraintTop_toTopOf="@+id/nav_host_fragment"/>
        <Constraint motion:layout_constraintEnd_toEndOf="parent" android:layout_width="wrap_content"
                    android:layout_height="wrap_content" motion:layout_constraintHorizontal_bias="0.5"
                    motion:layout_constraintStart_toStartOf="parent" android:id="@+id/prestarttime"
                    android:visibility="invisible"/>
        <Constraint android:id="@+id/frameLayout4" motion:layout_constraintEnd_toEndOf="parent"
                    android:layout_width="match_parent" android:layout_height="wrap_content"
                    android:layout_marginEnd="10dp" android:layout_marginStart="10dp"
                    motion:layout_constraintHorizontal_bias="0.5" motion:layout_constraintTop_toTopOf="parent"
                    motion:layout_constraintStart_toStartOf="parent" android:layout_marginTop="10dp"
                    android:visibility="invisible"/>
        <Constraint android:id="@+id/frameLayout4" motion:layout_constraintEnd_toEndOf="parent"
                    android:layout_width="match_parent" android:layout_height="wrap_content"
                    android:layout_marginEnd="10dp" android:layout_marginStart="10dp"
                    motion:layout_constraintHorizontal_bias="0.5" motion:layout_constraintTop_toTopOf="parent"
                    motion:layout_constraintStart_toStartOf="parent" android:layout_marginTop="10dp"
                    android:visibility="invisible"/>
        <Constraint android:id="@+id/frameLayout4" motion:layout_constraintEnd_toEndOf="parent"
                    android:layout_width="match_parent" android:layout_height="wrap_content"
                    android:layout_marginEnd="10dp" android:layout_marginStart="10dp"
                    motion:layout_constraintHorizontal_bias="0.5" motion:layout_constraintTop_toTopOf="parent"
                    motion:layout_constraintStart_toStartOf="parent" android:layout_marginTop="10dp"
                    android:visibility="invisible"/>
        <Constraint android:id="@+id/frameLayout4" motion:layout_constraintEnd_toEndOf="parent"
                    android:layout_width="match_parent" android:layout_height="wrap_content"
                    android:layout_marginEnd="10dp" android:layout_marginStart="10dp"
                    motion:layout_constraintHorizontal_bias="0.5" motion:layout_constraintTop_toTopOf="parent"
                    motion:layout_constraintStart_toStartOf="parent" android:layout_marginTop="10dp"
                    android:visibility="invisible"/>
        <Constraint android:id="@+id/frameLayout7" motion:layout_constraintEnd_toEndOf="parent"
                    android:layout_width="match_parent" android:layout_height="wrap_content"
                    motion:layout_constraintTop_toBottomOf="@id/frameLayout4" android:layout_marginEnd="10dp"
                    android:layout_marginStart="10dp" motion:layout_constraintHorizontal_bias="0.5"
                    motion:layout_constraintStart_toStartOf="parent" android:layout_marginTop="10dp"
                    android:visibility="invisible"/>
        <Constraint android:id="@+id/linearLayout3" motion:layout_constraintEnd_toEndOf="parent"
                    android:layout_width="match_parent" android:layout_height="match_parent"
                    motion:layout_constraintBottom_toBottomOf="parent" motion:layout_constraintHorizontal_bias="0.5"
                    motion:layout_constraintTop_toTopOf="parent" motion:layout_constraintStart_toStartOf="parent"
                    android:visibility="invisible"/>
        <Constraint android:id="@+id/linearLayout3" motion:layout_constraintEnd_toEndOf="parent"
                    android:layout_width="match_parent" android:layout_height="match_parent"
                    motion:layout_constraintBottom_toBottomOf="parent" motion:layout_constraintHorizontal_bias="0.5"
                    motion:layout_constraintTop_toTopOf="parent" motion:layout_constraintStart_toStartOf="parent"
                    android:visibility="invisible"/>
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint android:id="@+id/nav_view" android:layout_width="0dp" android:layout_height="0dp"
                    motion:layout_constraintBottom_toBottomOf="parent"
                    motion:layout_constraintLeft_toLeftOf="parent" motion:layout_constraintRight_toRightOf="parent"
                    android:visibility="invisible"
                    motion:layout_constraintTop_toBottomOf="@+id/nav_host_fragment"/>
        <Constraint android:id="@+id/nav_host_fragment" android:layout_width="match_parent"
                    android:layout_height="wrap_content" motion:layout_constraintBottom_toBottomOf="parent"
                    motion:layout_constraintLeft_toLeftOf="parent" motion:layout_constraintRight_toRightOf="parent"
                    motion:layout_constraintTop_toTopOf="parent" motion:layout_constraintHorizontal_bias="0.0"
                    android:layout_marginBottom="0dp"
                    android:visibility="invisible" android:layout_marginTop="50dp"
                    motion:layout_constraintVertical_bias="0.95"/>
        <Constraint motion:layout_editor_absoluteY="-78dp" motion:layout_editor_absoluteX="83dp"
                    android:id="@+id/imageView"/>
        <Constraint android:layout_height="wrap_content" motion:layout_constraintStart_toStartOf="parent"
                    motion:layout_constraintTop_toTopOf="parent" motion:layout_constraintHorizontal_bias="0.5"
                    motion:layout_constraintEnd_toEndOf="parent" android:layout_width="match_parent"
                    android:id="@+id/frameLayout" android:layout_marginTop="50dp" android:visibility="visible"
                    android:layout_marginLeft="20dp" android:layout_marginStart="20dp" android:layout_marginRight="20dp"
                    android:layout_marginEnd="20dp" motion:layout_constraintBottom_toTopOf="@+id/frameLayout2"
                    android:layout_marginBottom="20dp"/>
        <Constraint android:id="@+id/frameLayout2"
                    android:layout_width="match_parent" android:layout_height="wrap_content"
                    android:layout_marginTop="10dp" motion:layout_constraintTop_toBottomOf="@id/frameLayout4"
                    motion:layout_editor_absoluteX="10dp"
                    motion:layout_constraintStart_toStartOf="parent"
                    motion:layout_constraintEnd_toStartOf="@+id/nav_host_fragment"
                    motion:layout_constraintEnd_toEndOf="parent"/>
        <Constraint android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/frameLayout2"
                    motion:layout_constraintEnd_toEndOf="parent" motion:layout_constraintStart_toStartOf="parent"
                    motion:layout_constraintTop_toBottomOf="@id/frameLayout4"
                    android:layout_marginTop="10dp" motion:layout_editor_absoluteX="10dp"
                    motion:layout_constraintEnd_toStartOf="@+id/nav_host_fragment"
        />
        <Constraint motion:layout_constraintEnd_toEndOf="parent" android:layout_width="wrap_content"
                    android:layout_height="wrap_content" motion:layout_constraintHorizontal_bias="0.5"
                    motion:layout_constraintStart_toStartOf="parent" android:id="@+id/prestarttime"
                    android:visibility="visible"
                    motion:layout_constraintBottom_toTopOf="@+id/frameLayout"
                    motion:layout_editor_absoluteY="25dp" android:layout_marginBottom="10dp"/>
        <Constraint motion:layout_constraintEnd_toEndOf="parent" android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:layout_marginEnd="10dp"
                    android:layout_marginStart="10dp" motion:layout_constraintHorizontal_bias="0.5"
                    motion:layout_constraintTop_toTopOf="parent" motion:layout_constraintStart_toStartOf="parent"
                    android:layout_marginTop="10dp" android:id="@+id/frameLayout4" android:layout_marginLeft="10dp"
                    android:visibility="visible"/>
        <Constraint motion:layout_editor_absoluteY="236dp" android:id="@+id/frameLayout7"/>
        <Constraint motion:layout_constraintEnd_toEndOf="parent" android:layout_width="match_parent"
                    android:layout_height="wrap_content" motion:layout_constraintTop_toBottomOf="@id/frameLayout4"
                    android:layout_marginEnd="10dp" android:layout_marginStart="10dp"
                    motion:layout_constraintHorizontal_bias="0.5" motion:layout_constraintStart_toStartOf="parent"
                    android:layout_marginTop="10dp" android:id="@+id/frameLayout7" android:visibility="visible"/>
        <Constraint android:id="@+id/linearLayout3" motion:layout_constraintEnd_toEndOf="parent"
                    android:layout_width="match_parent" android:layout_height="match_parent"
                    motion:layout_constraintBottom_toBottomOf="parent" motion:layout_constraintHorizontal_bias="0.5"
                    motion:layout_constraintTop_toTopOf="parent" motion:layout_constraintStart_toStartOf="parent"
                    android:visibility="visible"/>
    </ConstraintSet>
</MotionScene>